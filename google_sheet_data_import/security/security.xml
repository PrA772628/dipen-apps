<odoo>

    <record model="ir.module.category" id="module_category_Google_api">
            <field name="name">Google Sheet API Access</field>
            <field name="description">Helps you to manage The API</field>
            <field name="sequence">10</field>
    </record>

    <record id="group_Api_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="google_sheet_data_import.module_category_Google_api"/>
    </record>

    <record id="group_Api_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="google_sheet_data_import.module_category_Google_api"/>
        <field name="implied_ids" eval="[(4, ref('group_Api_user'))]"/>
    </record>

</odoo>